{
  "expo": {
    "name": "LUDU_V2",
    "slug": "LUDU_V2",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/ludu_logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/ludu_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/ludu_logo.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/ludu_logo.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow LUDU to use your location."
        }
      ]
    ]
  }
}
